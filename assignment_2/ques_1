#include <iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main() {
    vector<int>start={1,3,0,5,8,5};
    vector<int>finish={2,4,6,7,9,9};
    int n=6;
    vector<pair<int,int>>c;
    for(int i=0;i<n;i++){
        c.push_back({finish[i],start[i]});
    }
    sort(c.begin(),c.end());
    int lasttime=c[0].first;
    int count=1;
    for(int i=1;i<n;i++){
        if(c[i].second>=lasttime){
            count++;
            lasttime=c[i].first;
        }
    }
    cout<<"max activities= "<<count;
    return 0;
}

